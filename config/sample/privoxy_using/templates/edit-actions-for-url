# bg=background, en="Enable", dis="Disable", noc="No Change", hdr=header
# 1=odd rows, 2=even rows, h=Table header
tr.hdr       { background-color: #999999 }
tr.bg1       { background-color: #eeeeee }
td.en1       { background-color: #ddffdd }
